{
  "name": "ele-easy-table",
  "version": "1.1.6",
  "private": false,
  "description": "基于Vue,ElementUI开发的一款表格组件，对ElementUI的el-table进行二次封装",
  "author": "vincentzyc",
  "keywords": [
    "component",
    "vue",
    "element-ui",
    "el-table"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentzyc/ele-easy-table"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name eleEasyTable --dest dist ./src/index.js"
  },
  "main": "dist/eleEasyTable.umd.min.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "dependencies": {
    "element-ui": "^2.4.5",
    "vue": "^2.6.10",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
