{
	"name": "ele-easy-table",
	"version": "1.0.5",
	"private": false,
	"description": "基于Vue,ElementUI开发的一款表格组件，对ElementUI的el-table进行二次封装",
	"author": "vincentzyc",
	"keywords": [
		"component",
		"vue",
		"element-ui",
		"el-table"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/vincentzyc/ele-easy-table"
	},
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"build-bundle": "vue-cli-service build --target lib --name eleEasyTable --dest dist ./src/index.js"
	},
	"main": "dist/eleEasyTable.common.js",
	"dependencies": {
		"core-js": "^2.6.5",
		"element-ui": "^2.4.5",
		"vue": "^2.6.6"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.5.0",
		"@vue/cli-plugin-eslint": "^3.5.0",
		"@vue/cli-service": "^3.5.0",
		"babel-eslint": "^10.0.1",
		"babel-plugin-component": "^1.1.1",
		"eslint": "^5.8.0",
		"eslint-plugin-vue": "^5.0.0",
		"vue-cli-plugin-element": "^1.0.1",
		"vue-template-compiler": "^2.5.21"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}